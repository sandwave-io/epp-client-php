{
    "name": "sandwave-io/epp-client-php",
    "description": "A base EPP Client that complies with RFC 5730, 5731, 5732, 5733, and 5734.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jesse Kramer",
            "email": "jesse@kramerventures.nl"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "nesbot/carbon": "^2.39",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.25",
        "phpunit/phpunit": "^7.5",
        "uptimeproject/php-cs-fixer-config": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "SandwaveIo\\EppClient\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SandwaveIo\\EppClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
